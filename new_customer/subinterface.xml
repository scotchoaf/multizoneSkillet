<entry name="{{ INTF_TRUST }}.{{ vlan_tag }}">
  <ipv6>
    <neighbor-discovery>
      <router-advertisement>
        <enable>no</enable>
      </router-advertisement>
    </neighbor-discovery>
  </ipv6>
  <ndp-proxy>
    <enabled>no</enabled>
  </ndp-proxy>
  <adjust-tcp-mss>
    <enable>no</enable>
  </adjust-tcp-mss>
  <ip>
    <entry name="{{ vlan_ip }}"/>
  </ip>
  <tag>{{ vlan_tag }}</tag>
  <comment>{{ customer_ID }}</comment>
</entry>

